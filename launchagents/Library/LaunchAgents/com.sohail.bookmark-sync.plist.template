<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.sohail.bookmark-sync</string>
    <key>ProgramArguments</key>
    <array>
        <string>${HOME}/Repos/experiments/assistant/bin/bookmark-sync-launchd</string>
    </array>
    <key>StartInterval</key>
    <integer>3600</integer>
    <key>WorkingDirectory</key>
    <string>${HOME}/Repos/experiments/assistant</string>
    <key>StandardOutPath</key>
    <string>/tmp/bookmark-sync.log</string>
    <key>StandardErrorPath</key>
    <string>/tmp/bookmark-sync.err</string>
    <key>EnvironmentVariables</key>
    <dict>
        <key>PATH</key>
        <string>/usr/local/bin:/opt/homebrew/bin:${HOME}/.bun/bin:/usr/bin:/bin</string>
        <key>HOME</key>
        <string>${HOME}</string>
    </dict>
    <key>RunAtLoad</key>
    <true/>
</dict>
</plist>
